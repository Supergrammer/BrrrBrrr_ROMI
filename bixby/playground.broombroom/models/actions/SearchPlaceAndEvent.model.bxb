action (SearchPlaceAndEvent) {
  type (Search)
  description (인증 코드와 가이드 타입을 받아 장소/행사 리스트를 반환합니다.)
  collect {
    input (authenticationCode) {
      type (AuthenticationCode)
      min (Required) max (One)
      default-init {
        intent {
          goal: CheckAuthenticationCode
        }
      }
    }
    input (guideType) {
      type (GuideType)
      min (Required) max (One)
    }
  }
  output (Item)
}
