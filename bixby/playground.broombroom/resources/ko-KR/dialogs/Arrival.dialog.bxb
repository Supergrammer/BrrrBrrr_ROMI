template-macro-def (Arrival) {
  params {
    param (commitOrder) {
      type (CommitOrder)
    }
  }
  content {
    template ("#{value(commitOrder.order.destination.destinationName)}에 도착했습니다!") {
          speech ("#{value(commitOrder.order.destination.destinationName)}에 도착했습니다! #{value(commitOrder.order.destination.destinationDescription)}")
        }
  }
}